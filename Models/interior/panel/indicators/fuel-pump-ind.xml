<?xml version="1.0"?>

<PropertyList>

 <path>blue_ind.ac</path>

 <animation>
  <object-name>light_on</object-name>
  <type>select</type>
  <condition>
	<and>
    <property>controls/fuel/tank/boost-pump</property>
   <greater-than>
    <property>/systems/ela07s/electrical/volts</property>
    <value>8.3</value>
   </greater-than>
  </and>
  </condition>
 </animation>

 <animation> 
  <type>material</type> 
  <object-name>light_on</object-name>
  <condition>
    	<and>
    <property>controls/fuel/tank/boost-pump</property>
   <greater-than>
    <property>/systems/ela07s/electrical/volts</property>
    <value>8.3</value>
   </greater-than>
  </and>
  </condition>

  <emission>
   <red>0.0</red>
   <green>0.2</green>
   <blue>1.0</blue>
  </emission>
 </animation>


</PropertyList>


