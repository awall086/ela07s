<?xml version="1.0"?>

<PropertyList>

 <path>std_switch.ac</path>

<!--	fuel pump switch		-->
<animation>
    <type>translate</type>
    <object-name>master_sw1</object-name>
    <property>controls/fuel/tank/boost-pump-switch</property>
    <factor>-0.005</factor>
    <axis>
        <x>1.0</x>
        <y>0.0</y>
        <z>0.0</z>
    </axis>
    <center>
        <x-m>0</x-m>
        <y-m>0</y-m>
        <z-m>0</z-m>
    </center>
</animation>
<animation>
   <type>pick</type>
   <object-name>master_sw1</object-name>
   <visible>true</visible>
   <action>
     <button>0</button>
     <repeatable>false</repeatable>
     <binding>
       <command>property-toggle</command>
       <property>controls/fuel/tank/boost-pump-switch</property>
     </binding>
 <!--    <binding>
         <command>nasal</command>
         <script>c152.click("master-bat")</script>
     </binding> -->
   </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>Fuel-Pump-switch</tooltip-id>
      <label>Fuel Pump: %s</label>
      <mapping>on-off</mapping>
      <property>controls/fuel/tank/boost-pump-switch</property>
   </binding>
   </hovered>
</animation>        

</PropertyList>


